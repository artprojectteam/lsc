{
  "name": "arc-boilerplate",
  "version": "1.0.1",
  "description": "総合的なウェブ用ボイラープレート",
  "keywords": [],
  "main": "",
  "author": "arc one",
  "scripts": {
    "start": "cross-env E=development M=watch npm-run-all -p pre:* b:* w:*",
    "build": "cross-env E=development M=once npm-run-all -s clean pre:* b:* webpack -s a:*",
    "release": "cross-env E=production M=once npm-run-all -s clean pre:* b:* webpack",
    "server": "NODE_ENV=development HOST=server babel-node bundler/node/server.js",
    "test": "cross-env NODE_ENV=test babel-node ./node_modules/karma/bin/karma start karma.conf.babel.js",
    "clean": "NODE_ENV=$E babel-node bundler/node/clean.js",
    "b:copy": "NODE_ENV=$E MODE=$M babel-node bundler/node/copy.js",
    "b:yaml2json": "NODE_ENV=$E MODE=$M babel-node bundler/node/yaml2json.js",
    "b:manifest": "NODE_ENV=$E MODE=$M babel-node bundler/node/manifest.js",
    "pre:stylusConst": "NODE_ENV=$E MODE=$M babel-node bundler/node/stylusConst.js",
    "webpack": "NODE_ENV=$E MODE=$M webpack",
    "w:webpack": "NODE_ENV=$E MODE=$M webpack --watch",
    "a:serviceWorker": "NODE_ENV=$E babel-node bundler/node/serviceWorker.js"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.8.0",
    "babel": ">= 7.1.0",
    "yarn": ">= 1.7.0"
  },
